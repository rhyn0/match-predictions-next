{
   "biome.enabled": true,
   "editor.defaultFormatter": "biomejs.biome",
   "biome.lspBin": "./node_modules/.bin/biome",
   "jest.enable": true,
   "jest.runMode": "on-save",
   "testing.openTesting": "neverOpen",
   "jest.outputConfig": {
      "revealOn": "error",
      "revealWithFocus": "test-results",
      "clearOnRun": "none"
   },
   "prettier.enable": false,
   "eslint.enable": false,
   "editor.tabSize": 4,
   "editor.formatOnSave": true,
   "inlineFold.regex": "(class|className)=[`'{\"]([^`'\"}]{36,})[`'\"}]",
   "files.exclude": {
      "**/.git": true,
      "**/.svn": true,
      "**/.hg": true,
      "**/CVS": true,
      "**/.DS_Store": true,
      "**/Thumbs.db": true
   },
   "[svg]": {
      "editor.defaultFormatter": "jock.svg"
   },
   "[json]": {
      "editor.quickSuggestions": {
         "strings": true
      },
      "editor.defaultFormatter": "biomejs.biome",
      "editor.suggest.insertMode": "replace"
   }
}